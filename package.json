{
  "name": "erpnextpopup",
  "version": "1.0.1",
  "description": "A Simple Library to Integrate Static Site with ERPNext",
  "keywords": [
    "ERPNext",
    "POS",
    "static-site",
    "contactus",
    "lead"
  ],
  "homepage": "https://github.com/Grynn-GmbH/erpnextpopup",
  "bugs": {
    "url": "https://github.com/Grynn-GmbH/erpnextpopup",
    "email": "sales@grynn.in"
  },
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "parcel template/index.html --port 5000 --out-dir www",
    "build:website": "parcel build template/index.html --port 5000 --out-dir www",
    "build": "npm run clean && tsc",
    "doc": "typedoc  --out docs src/*",
    "clean": "rimraf dist && rimraf www",
    "fix": "eslint --fix",
    "major": "yarn version --major",
    "minor": "yarn version --minor",
    "patch": "yarn version --patch"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "prettier": "^2.3.0",
    "prettier-eslint": "^12.0.0",
    "rimraf": "^3.0.2",
    "typedoc": "^0.20.36",
    "typescript": "^4.3.2"
  },
  "files": [
    "./dist",
    "LICENSE",
    "README.md",
    "package.json"
  ]
}
